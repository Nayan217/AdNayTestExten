{
  "name": "adhinayan",
  "displayName": "AdNayTestExten",
  "description": "Test Extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "publisher": "AdhiNayExtensions",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [{
      "command": "adhinayan.performAction",
      "title": "Process Selected Items"
    }],
    "viewsContainers": {
      "activitybar": [{
        "id": "adhinayan-activity",
        "title": "adhinayan",
        "icon": "images/NA.svg"
      }]
    },
    "views": {
      "adhinayan-activity": [{
        "type": "webview",
        "id": "adhinayan-container",
        "name": "adhinayan View",
        "icon": "images/NA.svg"
      }]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "activate": "webpack && code --extensionDevelopmentPath=.",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "ts-loader": "^9.5.2",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}